<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TargetAchievedForm__c</fullName>
    <description>This formula calculates the target achieved percentage based on the Won revenue from the opportunity divided by the set target.</description>
    <externalId>false</externalId>
    <formula>IF (  SalesTarget__c  = 0, 0 , WonRevenueTrig__c / SalesTarget__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Target Achieved %</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
